<table>
  <tr>
    <th>Name</th>
   
    
  </tr>

<% @categorys.each do |c| %>
  <tr>
    <td><%= c.name %></td>
    
    <td><%= link_to 'Show', category_path(c) %></td>
    <td><%= link_to 'Edit', edit_category_path(c) %></td>
    <td><%= link_to 'Destroy', category_path(c), :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>